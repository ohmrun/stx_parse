{
  "build" : {
    "-cp" : "src/main/haxe",
    "-lib" : [
      "stx_pico","stx_nano", "stx_ds", "stx_assert", "stx_fn", "stx_test", "fletcher", "hre"
    ],
    "-D" : [
      
    ],
    "unit"  : {
      "--macro" : [
        "include('stx.Parse',true)",      
        "include('stx.parse',true)"
      ],
      "interp" : {
        "--interp"  : true
      },
      "cpp"  : {
        "--cpp" : "build/unit/cpp"
      }
    },
    "test" : {
      "-main" : "stx.parse.Test",
      "interp" : {
        "--interp" : true
      },
      "js" : {
        "-lib" : "hxnodejs",
        "--js" : "build/test/js/main.js"
      },
      "hl" : {
        "--hl" : "build/test/hl/main.hl"
      },
      "cpp" : {
        "--cpp" : "build/test/cpp"
      }
    },
    "yaml" : {
      "-main" : "stx.yaml.Test",
      "interp" : {
        "--interp" : true
      }
    },
    "json" : {
      "-D" : [
        "stx.parse.switches.debug=false"
      ],
      "-resource" : [
        "src/main/haxe/stx/parse/term/simple.json@simple",
        "haxe.json@haxe"
      ],
      "test" : {
        "-main"   : "stx.parse.test.JsonTest",
        "interp" : {
          "--interp" : true
        }
      }
    }
  },
  "run" : {
    "test.js" : [
      ["hx","build","test/js"],
      ["node","build/test/js/main.js"]
    ],
    "test.hl" : [
      ["hx","build","test/hl"],
      ["hl","build/test/hl/main.hl"]
    ],
    "test.cpp" : [
      ["hx","build","test/cpp"]
    ]
  }
}